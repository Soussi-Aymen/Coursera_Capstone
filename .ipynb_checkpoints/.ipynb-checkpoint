   "source": [
    "coworking_venues = coworking_space_results['response']['venues']\n",
    "coworking_dataframe = json_normalize(coworking_venues)\n",
    "\n",
    "\n",
    "coworking_filtered_columns = ['name', 'categories'] + [col for col in coworking_dataframe.columns if col.startswith('location.')] + ['id']\n",
    "coworking_dataframe_filtered = coworking_dataframe.loc[:, coworking_filtered_columns]\n",
    "\n",
    "def get_category_type(row):\n",
    "    try:\n",
    "        categories_list = row['categories']\n",
    "    except:\n",
    "        categories_list = row['venue.categories']\n",
    "        \n",
    "    if len(categories_list) == 0:\n",
    "        return None\n",
    "    else:\n",
    "        \n",
    "        return categories_list[0]['name']\n",
    "coworking_dataframe_filtered['categories'] = coworking_dataframe_filtered.apply(get_category_type, axis=1) \n",
    "\n",
    "coworking_dataframe_filtered.columns = [column.split('.')[-1] for column in coworking_dataframe_filtered.columns]\n",
    "\n",
    "print(coworking_dataframe['name'].head(20))\n",
    "coworking_dataframe.head(20)"
   ]
  },
